; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GameJam78"
run/main_scene="res://UserInterface/Startup/SplashScreen/SplashScreen.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")

[audio]

buses/default_bus_layout="res://Config/Settings/AudioBus.tres"

[autoload]

SettingsManager="*res://Globals/SettingsManager.gd"
DungeonManager="*res://Globals/DungeonManager.gd"
CombatManager="*res://Globals/CombatManager.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/stretch/mode="canvas_items"

[editor]

naming/scene_name_casing=1
naming/script_name_casing=1

[editor_plugins]

enabled=PackedStringArray("res://addons/SafeResourceLoader/plugin.cfg", "res://addons/StateMachine/plugin.cfg")

[file_customization]

folder_colors={
"res://Assets/": "red",
"res://Common/": "orange",
"res://Config/": "yellow",
"res://Entities/": "purple",
"res://Environment/": "pink",
"res://Globals/": "gray",
"res://Stages/": "blue",
"res://UserInterface/": "green",
"res://addons/": "gray"
}

